      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@expo/webpack-config": "^0.16.19",
    "expo": "^44.0.6",
    "jest": "^26.6.3",
    "jest-fuzz": "^0.1.2",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/jest": "^29.1.2",
    "babel-plugin-transform-flow-to-gen": "^0.0.13"
  },
  "private": true
